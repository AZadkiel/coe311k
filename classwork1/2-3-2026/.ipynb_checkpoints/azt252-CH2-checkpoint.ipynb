    "import numpy as np\n",
    "\n",
    "def BinaryReader(x):\n",
    "    binaryList = []\n",
    "    \n",
    "    while x > 0:\n",
    "        number = x % 2\n",
    "        binaryList.append(number)\n",
    "        x = x // 2\n",
    "        binaryList.reverse()\n",
    "    return binaryList\n",
